alt + Return
      samedir

alt + Escape
      pkill -USR1 -x sxhkd

alt + shift + w
      $BROWSER

Print
      maim screenshot-$(date '+%d%m%y-%H:%M').png

alt + Print
      record

alt + F1
      shortcuts

alt + F2
      $TERMINAL -e htop

alt + F3
      displayselect

alt + F4
      nmtui

alt + F5
      icons

alt + F10
      showclip
